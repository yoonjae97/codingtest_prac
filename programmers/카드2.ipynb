   "source": [
    "from collections import deque\n",
    "\n",
    "n = int(input())\n",
    "lst = deque([i for i in range(1, n + 1)])\n",
    "\n",
    "while len(lst) > 1:\n",
    "    lst.popleft()\n",
    "    now = lst.popleft()\n",
    "    lst.append(now)\n",
    "\n",
    "print(lst[0])\n"
   ]
  }
