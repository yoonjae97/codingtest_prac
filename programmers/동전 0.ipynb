   "source": [
    "n, k = map(int, input().split())\n",
    "coins = [int(input()) for i in range(n)]\n",
    "coins.sort(reverse = True)\n",
    "result = 0\n",
    "\n",
    "while k != 0:\n",
    "    for coin in coins:\n",
    "        if coin > k:\n",
    "            continue\n",
    "        else:\n",
    "            result += (k//coin)\n",
    "\n",
    "            k %= coin\n",
    "\n",
    "print(result)\n",
    "\n"
   ]
  }
 ],
